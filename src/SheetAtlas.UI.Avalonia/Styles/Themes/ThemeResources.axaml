<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Light Theme Colors -->
  <ResourceDictionary x:Key="LightTheme">

    <!-- Brand Colors -->
    <SolidColorBrush x:Key="PrimaryNavyBlue" Color="#1E3A5F"/>
    <SolidColorBrush x:Key="AccentOrange" Color="#FF6B35"/>
    <SolidColorBrush x:Key="SecondaryBlue" Color="#4A90E2"/>

    <!-- Backgrounds -->
    <SolidColorBrush x:Key="MainBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="TertiaryBackground" Color="#F1F3F5"/>
    <SolidColorBrush x:Key="CardBackground" Color="#FFFFFF"/>

    <!-- Text Colors -->
    <SolidColorBrush x:Key="PrimaryText" Color="#212529"/>
    <SolidColorBrush x:Key="SecondaryText" Color="#6C757D"/>
    <SolidColorBrush x:Key="DisabledText" Color="#ADB5BD"/>
    <SolidColorBrush x:Key="LinkText" Color="#4A90E2"/>

    <!-- Neutral Grays -->
    <SolidColorBrush x:Key="Gray100" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="Gray200" Color="#E9ECEF"/>
    <SolidColorBrush x:Key="Gray300" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="Gray400" Color="#CED4DA"/>
    <SolidColorBrush x:Key="Gray500" Color="#ADB5BD"/>
    <SolidColorBrush x:Key="Gray600" Color="#6C757D"/>
    <SolidColorBrush x:Key="Gray700" Color="#495057"/>
    <SolidColorBrush x:Key="Gray800" Color="#343A40"/>
    <SolidColorBrush x:Key="Gray900" Color="#212529"/>

    <!-- State Colors -->
    <SolidColorBrush x:Key="SuccessColor" Color="#28A745"/>
    <SolidColorBrush x:Key="SuccessBackground" Color="#D4EDDA"/>
    <SolidColorBrush x:Key="WarningColor" Color="#FFC107"/>
    <SolidColorBrush x:Key="WarningBackground" Color="#FFF3CD"/>
    <SolidColorBrush x:Key="ErrorColor" Color="#DC3545"/>
    <SolidColorBrush x:Key="ErrorBackground" Color="#F8D7DA"/>
    <SolidColorBrush x:Key="InfoColor" Color="#17A2B8"/>
    <SolidColorBrush x:Key="InfoBackground" Color="#D1ECF1"/>

    <!-- Interactive States -->
    <SolidColorBrush x:Key="HoverBackground" Color="#FF6B35" Opacity="0.15"/>
    <SolidColorBrush x:Key="PressedBackground" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="SelectedBackground" Color="#E3F2FD"/>
    <SolidColorBrush x:Key="ActiveBackground" Color="#1E3A5F" Opacity="0.1"/>
    <SolidColorBrush x:Key="FocusBorder" Color="#FF6B35"/>

    <!-- Borders and Dividers -->
    <SolidColorBrush x:Key="BorderColor" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="SubtleBorder" Color="#E9ECEF"/>

    <!-- Search Specific -->
    <SolidColorBrush x:Key="SearchQueryBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SearchQueryText" Color="#1A202C"/>
    <SolidColorBrush x:Key="SearchQueryBorder" Color="#1E3A5F"/>
    <SolidColorBrush x:Key="FileGroupBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="FileGroupBorder" Color="#1E3A5F"/>
    <SolidColorBrush x:Key="SheetGroupBackground" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="SheetGroupBorder" Color="#1E3A5F"/>
    <SolidColorBrush x:Key="HighlightYellow" Color="#FFF3CD"/>

    <!-- Comparison Colors - Subtle tints for readability -->
    <SolidColorBrush x:Key="ComparisonMatchBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComparisonDifferentBackground" Color="#FFF8F0"/>
    <SolidColorBrush x:Key="ComparisonNewBackground" Color="#F8FDF9"/>
    <SolidColorBrush x:Key="ComparisonMissingBackground" Color="#FEF8F8"/>
    <SolidColorBrush x:Key="ComparisonHeaderBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComparisonHeaderBorder" Color="#1E3A5F"/>
    <SolidColorBrush x:Key="ComparisonAlternateRow" Color="#F8F9FA"/>

    <!-- MenuFlyout/Submenu Specific Resources for Light Theme -->
    <SolidColorBrush x:Key="MenuFlyoutPresenterBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MenuFlyoutPresenterBorderBrush" Color="#DEE2E6"/>
    <Thickness x:Key="MenuFlyoutPresenterBorderThemeThickness">1</Thickness>
    <Thickness x:Key="MenuFlyoutPresenterThemePadding">4</Thickness>
    <CornerRadius x:Key="OverlayCornerRadius">4</CornerRadius>

    <!-- System Control Colors for Light Theme (FluentTheme compatibility) -->
    <Color x:Key="SystemAltHighColor">#FFFFFF</Color>
    <Color x:Key="SystemAltLowColor">#FFFFFF</Color>
    <Color x:Key="SystemAltMediumColor">#FFFFFF</Color>
    <Color x:Key="SystemAltMediumHighColor">#FFFFFF</Color>
    <Color x:Key="SystemAltMediumLowColor">#FFFFFF</Color>
    <Color x:Key="SystemBaseHighColor">#000000</Color>
    <Color x:Key="SystemBaseLowColor">#CCCCCC</Color>
    <Color x:Key="SystemBaseMediumColor">#898989</Color>
    <Color x:Key="SystemBaseMediumHighColor">#5D5D5D</Color>
    <Color x:Key="SystemBaseMediumLowColor">#737373</Color>
    <Color x:Key="SystemChromeAltLowColor">#5D5D5D</Color>
    <Color x:Key="SystemChromeBlackHighColor">#000000</Color>
    <Color x:Key="SystemChromeBlackLowColor">#CCCCCC</Color>
    <Color x:Key="SystemChromeBlackMediumColor">#5D5D5D</Color>
    <Color x:Key="SystemChromeBlackMediumLowColor">#898989</Color>
    <Color x:Key="SystemChromeDisabledHighColor">#CCCCCC</Color>
    <Color x:Key="SystemChromeDisabledLowColor">#898989</Color>
    <Color x:Key="SystemChromeGrayColor">#737373</Color>
    <Color x:Key="SystemChromeHighColor">#CCCCCC</Color>
    <Color x:Key="SystemChromeLowColor">#ECECEC</Color>
    <Color x:Key="SystemChromeMediumColor">#E6E6E6</Color>
    <Color x:Key="SystemChromeMediumLowColor">#ECECEC</Color>
    <Color x:Key="SystemChromeWhiteColor">#FFFFFF</Color>
    <Color x:Key="SystemListLowColor">#E6E6E6</Color>
    <Color x:Key="SystemListMediumColor">#CCCCCC</Color>
    <Color x:Key="SystemRegionColor">#FFFFFF</Color>
    <Color x:Key="SystemErrorTextColor">#DC3545</Color>

    <!-- System Control Brushes for Light Theme -->
    <SolidColorBrush x:Key="SystemControlBackgroundAltHighBrush" Color="{DynamicResource SystemAltHighColor}"/>
    <SolidColorBrush x:Key="SystemControlBackgroundAltMediumHighBrush" Color="{DynamicResource SystemAltMediumHighColor}"/>
    <SolidColorBrush x:Key="SystemControlBackgroundChromeMediumLowBrush" Color="{DynamicResource SystemChromeMediumLowColor}"/>
    <SolidColorBrush x:Key="SystemControlForegroundBaseHighBrush" Color="{DynamicResource SystemBaseHighColor}"/>
    <SolidColorBrush x:Key="SystemControlForegroundBaseMediumBrush" Color="{DynamicResource SystemBaseMediumColor}"/>
    <SolidColorBrush x:Key="SystemControlBackgroundBaseLowBrush" Color="{DynamicResource SystemBaseLowColor}"/>
    <SolidColorBrush x:Key="SystemControlDisabledBaseMediumLowBrush" Color="{DynamicResource SystemBaseMediumLowColor}"/>
    <SolidColorBrush x:Key="SystemControlHighlightBaseHighBrush" Color="#FF6B35"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentLowBrush" Color="#E3F2FD"/>
    <SolidColorBrush x:Key="SystemControlHighlightListLowBrush" Color="#F1F3F5"/>
    <SolidColorBrush x:Key="SystemControlHighlightListAccentMediumBrush" Color="#D1ECFE"/>
    <SolidColorBrush x:Key="SystemControlHighlightAltBaseHighBrush" Color="#000000"/>
    <SolidColorBrush x:Key="SystemControlTransparentBrush" Color="Transparent"/>

    <!-- ToolTip Resources for Light Theme -->
    <SolidColorBrush x:Key="ToolTipBackground" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="ToolTipForeground" Color="#212529"/>
    <SolidColorBrush x:Key="ToolTipBorderBrush" Color="#DEE2E6"/>
    <Thickness x:Key="ToolTipBorderThemeThickness">1</Thickness>
    <CornerRadius x:Key="ToolTipCornerRadius">3</CornerRadius>

    <!-- ComboBox Resources for Light Theme -->
    <SolidColorBrush x:Key="ComboBoxBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComboBoxForeground" Color="#212529"/>
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="ComboBoxDropDownBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComboBoxPopupBorderBrush" Color="#DEE2E6"/>

    <!-- Button Resources for Light Theme -->
    <SolidColorBrush x:Key="ButtonBackground" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#212529"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#E9ECEF"/>
    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#DEE2E6"/>

    <!-- TextBox Resources for Light Theme -->
    <SolidColorBrush x:Key="TextControlBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TextControlForeground" Color="#212529"/>
    <SolidColorBrush x:Key="TextControlBorderBrush" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="TextControlPlaceholderForeground" Color="#6C757D"/>

    <!-- ListBox Resources for Light Theme -->
    <SolidColorBrush x:Key="ListBoxBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ListBoxBorderBrush" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="ListBoxItemBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="ListBoxItemBackgroundPointerOver" Color="#F1F3F5"/>
    <SolidColorBrush x:Key="ListBoxItemBackgroundSelected" Color="#E3F2FD"/>
    <Thickness x:Key="ListBoxBorderThemeThickness">1</Thickness>

    <!-- ScrollViewer/ScrollBar Resources for Light Theme -->
    <SolidColorBrush x:Key="ScrollBarBackground" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="ScrollBarThumbBackground" Color="#ADB5BD"/>
    <SolidColorBrush x:Key="ScrollBarThumbBackgroundPointerOver" Color="#6C757D"/>
    <SolidColorBrush x:Key="ScrollBarThumbBackgroundPressed" Color="#495057"/>

    <!-- TreeView Resources for Light Theme -->
    <SolidColorBrush x:Key="TreeViewBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TreeViewBorderBrush" Color="#DEE2E6"/>
    <SolidColorBrush x:Key="TreeViewItemBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="TreeViewItemBackgroundPointerOver" Color="#F1F3F5"/>
    <SolidColorBrush x:Key="TreeViewItemBackgroundSelected" Color="#E3F2FD"/>

  </ResourceDictionary>

